<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="İstenen Gruplari Çalıştirma">
    <test name="Smoke Test">
        <groups>
            <run>
                <include name="minireg1"></include>
                <exclude name="smoke"></exclude>
            </run>
        </groups>
        
        <packages>
            <package name="tests.*"></package>
        </packages>

    </test>
</suite>